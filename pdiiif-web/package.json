{
  "name": "pdiiif-web",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "compile": "cross-env NODE_ENV=production vite build",
    "check": "svelte-check --human && tsc --noEmit",
    "watch:svelte": "svelte-check --human --watch",
    "watch:ts": "tsc --noEmit --watch",
    "start": "run-p watch:* dev",
    "build": "run-s check compile",
    "serve": "sirv dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbaiter/pdiiif.git"
  },
  "keywords": [
    "iiif",
    "pdf"
  ],
  "author": "Johannes Baiter <johannes.baiter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbaiter/pdiiif/issues"
  },
  "homepage": "https://github.com/jbaiter/pdiiif/tree/pdiiif-web#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "@types/wicg-file-system-access": "^2020.9.2",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": ">=8.2.14 <9.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.44.1",
    "svelte-check": "^2.2.9",
    "svelte-preprocess": "^4.7.4",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pdiiif": "workspace:../pdiiif-lib",
    "svelte-i18n": "^3.3.9"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "browserslist": [
    "defaults"
  ]
}
