{
  "name": "pdiiif-web",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "compile": "cross-env NODE_ENV=production vite build",
    "check": "svelte-check --human && tsc --noEmit",
    "watch:svelte": "svelte-check --human --watch",
    "watch:ts": "tsc --noEmit --watch",
    "start": "run-p watch:* dev",
    "build": "run-s check compile",
    "serve": "sirv dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbaiter/pdiiif.git"
  },
  "keywords": [
    "iiif",
    "pdf"
  ],
  "author": "Johannes Baiter <johannes.baiter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbaiter/pdiiif/issues"
  },
  "homepage": "https://github.com/jbaiter/pdiiif/tree/pdiiif-web#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^2.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/wicg-file-system-access": "^2020.9.5",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
  },
  "dependencies": {
    "@atlas-viewer/iiif-image-api": "^2.0.5",
    "@iiif/presentation-3": "^1.0.6",
    "@iiif/vault": "^0.9.18",
    "@iiif/vault-helpers": "^0.9.6",
    "@sentry/browser": "^7.4.1",
    "@sentry/tracing": "^7.4.1",
    "@types/ua-parser-js": "^0.7.36",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "pdiiif": "workspace:^0.1.0",
    "svelte-i18n": "^3.4.0",
    "ua-parser-js": "^1.0.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "browserslist": [
    "defaults"
  ]
}
