{
  "name": "pdiiif-web",
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "watch": "tsc --watch",
    "start": "nodemon dist/server.js",
    "dev": "run-p watch start",
    "format": "prettier -w ./src",
    "lint": "eslint . --ext .ts && tsc --noemit",
    "pretest": "tsc --noemit",
    "posttest": "eslint . --ext .ts",
    "prepack": "rm -rf dist && tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbaiter/pdiiif.git"
  },
  "keywords": [
    "iiif",
    "pdf"
  ],
  "author": "Johannes Baiter <johannes.baiter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbaiter/pdiiif/issues"
  },
  "homepage": "https://github.com/jbaiter/pdiiif/tree/pdiiif-api#readme",
  "devDependencies": {
    "@types/accept-language-parser": "^1.5.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsbn": "^1.2.29",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.6.1",
    "@types/node-fetch": "^2.5.12",
    "@types/qrcode-svg": "^1.1.1",
    "@types/sanitize-html": "^2.5.0",
    "@types/wicg-file-system-access": "^2020.9.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@atlas-viewer/iiif-image-api": "^2.0.5",
    "@iiif/parser": "^1.0.10",
    "@iiif/presentation-3": "^1.0.6",
    "@iiif/vault": "^0.9.18",
    "@iiif/vault-helpers": "^0.9.5",
    "@sentry/node": "^6.17.9",
    "@sentry/tracing": "^6.17.9",
    "@wesleytodd/openapi": "^0.1.0",
    "accept-language-parser": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.4.1",
    "handlebars": "^4.7.7",
    "ip-cidr": "^3.0.4",
    "lodash": "^4.17.21",
    "manifesto.js": "^4.2.4",
    "mnemonist": "^0.39.0",
    "node-fetch": "^2.6.1",
    "p-queue": "6.6.2",
    "pdiiif": "workspace:../pdiiif-lib",
    "pdiiif-web": "workspace:../pdiiif-web",
    "prom-client": "^14.0.1",
    "puppeteer": "^15.3.0",
    "qrcode-svg": "^1.1.0",
    "sanitize-html": "^2.5.3",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": ">=16.15.0"
  },
  "engineStrict": true,
  "main": "./dist/server.js",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    }
  }
}
